{
    "name": "jjcsinke/laravel-cms",
    "description": "CMS Module for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JJCS\\CMS\\": "backend",
            "JJCS\\Tests\\": "tests",
            "Database\\Factories\\JJCS\\CMS\\Models\\": "database/factories",
            "Database\\Seeders\\": "database/seeders"
        }
    },
    "authors": [
        {
            "name": "Jan-Jaap",
            "email": "mail@mrpaaj.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "spatie/laravel-permission": "^6.9"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JJCS\\CMS\\Providers\\CmsServiceProvider"
            ],
            "aliases": {
                "CMS": "JJCS\\CMS\\Facades\\CMSFacade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.3"
    }
}
